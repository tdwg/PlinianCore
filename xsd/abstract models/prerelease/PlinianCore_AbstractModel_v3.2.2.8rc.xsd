<!--description: The Plinian Core  is structured set of concepts  that can be used to describe different aspects of biological species information. Under "biological species Information" all kinds of properties or traits related to taxa—biological and non-biological—are included. Thus, for instance, terms pertaining descriptions, legal aspects, conservation, management, demographics, nomenclature, or related resources are incorporated. -->
<!--  publisher: Plinian Core TDWG Task Group; modified to point to __GISIN___ XSDs  in GitHub/TDWG  29 jun 2018 -->
<!-- v3.2.2 modified:2018-07-10: -->
<!-- v3.2.4 Term Strategy renamed to TrophicStrategy; term StrategyRemarks renamed to TrophicStrategyRemarks; v3.2.3 was an interin version never published. F. Pando   -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:plic="http://www.gbif.es/PliC/" xmlns:dc="http://purl.org/dc/terms/" xmlns:dcelem="http://purl.org/dc/elements/1.1/" xmlns:dwc="http://rs.tdwg.org/dwc/terms/" xmlns:abcd="http://www.tdwg.org/schemas/abcd/2.06" xmlns:tcs="http://www.tdwg.org/schemas/tcs/1.01" xmlns:simple="http://rs.tdwg.org/dwc/xsd/simpledarwincore/" xmlns:gisin="http://www.gisin.org/gisin/SpeciesStatus" xmlns:eol="http://www.eol.org/transfer/content/0.3" xmlns:eml="eml://ecoinformatics.org/eml-2.1.1" xmlns:sql="urn:schemas-microsoft-com:mapping-schema" xmlns:ns1="https://github.com/tdwg/dwc/tree/master/xsd">
	<xs:import namespace="http://www.tdwg.org/schemas/abcd/2.06" schemaLocation="http://rs.tdwg.org/abcd/2.06/ABCD_2.06.xsd"/>
	<xs:import namespace="http://www.tdwg.org/schemas/tcs/1.01" schemaLocation="https://raw.githubusercontent.com/tdwg/tcs/master/TCS101/v101.xsd"/>
	<xs:import namespace="http://www.gisin.org/gisin/SpeciesStatus" schemaLocation="https://raw.githubusercontent.com/tdwg/gisin/master/xsd/SpeciesStatus.xsd"/>
	<xs:import namespace="http://rs.tdwg.org/dwc/terms/" schemaLocation="https://raw.githubusercontent.com/tdwg/PlinianCore/master/xsd/abstract%20models/stable%20version/tdwg_dwc_extensions.xsd"/>
	<xs:import namespace="eml://ecoinformatics.org/eml-2.1.1" schemaLocation="https://raw.githubusercontent.com/tdwg/PlinianCore/master/xsd/abstract%20models/stable%20version/eml.xsd"/>
	<xs:import namespace="http://www.eol.org/transfer/content/0.3" schemaLocation="https://raw.githubusercontent.com/tdwg/PlinianCore/master/xsd/abstract%20models/stable%20version/content_0_3.xsd"/>
	<xs:import namespace="http://purl.org/dc/terms/" schemaLocation="http://dublincore.org/schemas/xmls/qdc/dcterms.xsd"/>
	<xs:import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="http://dublincore.org/schemas/xmls/qdc/dc.xsd"/>
	<xs:element name="Dataset">
		<xs:annotation>
			<xs:documentation>Root element from which all other elements will branch from</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Metadata">
					<xs:annotation>
						<xs:documentation>Information about the collections of records. This is an extension of the metadata of eml-gbif-profile.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TaxonRecord" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--  =============================================   -->
	<!--Elements-->
	<!--  =============================================   -->
	<xs:element name="Abstract" type="xs:string"/>
	<xs:element name="AncillaryData" type="eol:dataObjectBaseType">
		<xs:annotation>
			<xs:documentation> </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AnnualCycleAtomized" type="AnnualCycleAtomizedType"/>
	<xs:element name="AnnualCycles" type="AnnualCyclesType"/>
	<xs:element name="annualCycleUnstructured" type="xs:string"/>
	<xs:element name="DistributionAtomized" type="DistributionAtomizedType"/>
	<xs:element name="Audience">
		<xs:annotation>
			<xs:documentation>Repetitive concept where the target audiences are written one by one. (Predetermined list: 01. Expert (High). 02. Knowledgeable (Medium). 03. General (Low). </xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Expert">
					<xs:annotation>
						<xs:documentation>Professionals on Biodiversity (researchers, decision makers, students of higher levels, etc.). By training and experience they have a good knowledge of scientific terminology and taxonomic and nomenclatural issues.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="Knowledgeable">
					<xs:annotation>
						<xs:documentation>Public interest in the knowledge of living organisms but whose training and experience is not directly linked to knowledge of biodiversity, or to be enjoined, their training has not been directly related to biodiversity (professionals from other areas of knowledge, students, amateurs, etc.). Your level of interest is high but not usually use and know the terminology, taxonomy and scientific nomenclature.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="General">
					<xs:annotation>
						<xs:documentation>General Public (anyone interested in living organisms). Persons interested on biodiversity, frequently or timely, but lack the training, expertise or basic knowledge on terminology, taxonomy or scientific nomenclature.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AudiencesUnstructured" type="xs:string">
		<xs:annotation>
			<xs:documentation>List audience codes separated by means of comma or /.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BaseElements" type="BaseElementsType"/>
	<xs:element name="Behavior" type="BehaviorType">
		<xs:annotation>
			<xs:documentation>Responses, reactions or movements made by an organism in a particular situation.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BehaviorAtomized" type="DetailType"/>
	<xs:element name="BehaviorUnstructured" type="xs:string"/>
	<xs:element name="BriefDescription">
		<xs:annotation>
			<xs:documentation>Brief description which reflect only the diagnostic characters, i.e. those that distinguish a taxon from other related or close taxa.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CommonNamesAtomized" type="CommonNamesAtomizedType"/>
	<xs:element name="CommonNames" type="CommonNamesType"/>
	<xs:element name="DemographyAndThreat" type="DemographyAndThreatType"/>
	<xs:element name="DemographyAndThreatUnstructured" type="xs:string"/>
	<xs:element name="DetailAtomized" type="DetailType"/>
	<xs:element name="DetailUnstructured">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DirectThreats" type="DirectThreatsType">
		<xs:annotation>
			<xs:documentation>Direct threats are the proximate human activities or processes that have caused, are causing, or may cause the destruction, degradation, and/or impairment of biodiversity targets (e.g., unsustainable fishing or logging). Direct threats are synonymous with sources of stress and proximate pressures. Threats can be past (historical), ongoing, and/or likely to occur in the future.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DirectThreatAtomized" type="DetailType"/>
	<xs:element name="DirectThreatUnstructured" type="xs:string"/>
	<xs:element name="Dispersal" type="DispersalType"/>
	<xs:element name="DispersalAtomized" type="DispersalAtomizedType"/>
	<xs:element name="DispersalUnstructured" type="xs:string"/>
	<xs:element name="Distribution" type="DistributionType"/>
	<xs:element name="DistributionUnstructured" type="xs:string"/>
	<xs:element name="EcologicalSignificance" type="EcologicalSignificanceType">
		<xs:annotation>
			<xs:documentation>Ecological importance of the taxon. (Placeholder)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EcologicalSignificanceAtomized" type="DetailType"/>
	<xs:element name="EcologicalSignificanceUnstructured" type="xs:string"/>
	<xs:element name="Endemic" type="EndemicType"/>
	<xs:element name="EndemicAtomized" type="EndemicAtomizedType">
		<xs:annotation>
			<xs:documentation>List of areas structured and categorized the degree of safety.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EnvironmentalEnvelope" type="EnvironmentalEnvelopeType"/>
	<xs:element name="EnvironmentalEnvelopeAtomized" type="DetailType"/>
	<xs:element name="EnvironmentalEnvelopeUnstructured" type="xs:string"/>
	<xs:element name="EndemicUnstructured" type="xs:string">
		<xs:annotation>
			<xs:documentation>List of areas presented in an unstructured and categorized the degree of safety.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Feeding" type="FeedingType"/>
	<xs:element name="FeedingAtomized" type="FeedingAtomizedType"/>
	<xs:element name="FeedingUnstructured" type="xs:string"/>
	<xs:element name="FullDescription" type="FullDescriptionType">
		<xs:annotation>
			<xs:documentation>Complete and detailed account of the taxonomic and systematic characters, i.e. those that define and allow to recognize an organism.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FullDescriptionAtomized" type="DetailType"/>
	<xs:element name="FullDescriptionUnstructured" type="xs:string"/>
	<xs:element name="HabitatAtomized" type="DetailType"/>
	<xs:element name="HabitatAndDistribution" type="HabitatAndDistributionType"/>
	<xs:element name="HabitatAndDistributionUnstructured" type="xs:string"/>
	<xs:element name="Habitats" type="HabitatsType"/>
	<xs:element name="HabitatUnstructured" type="xs:string"/>
	<xs:element name="Hierarchy" type="HierarchyType"/>
	<xs:element name="InteractionAtomized" type="InteractionAtomizedType"/>
	<xs:element name="IdentificationKeys" type="IdentificationKeysType">
		<xs:annotation>
			<xs:documentation>Tool for the identification of organisms, comprising a series of steps, each of which represents a question about one or more characters of the organism. Each step offers multiple alternatives, choosing one of them will lead us to the next step until ending, finally, in the name of the organism. Most printed keys have only two alternatives, so called dichotomous keys. A taxon includes a key or an identification of key in the subordinate taxons. 
For example: 
If we are in a register of a genre, this register would include the keys of the species that this register includes, and if it´s one specie, then it would include the key of the infraspecific rank (For plants: subspecies, variety forms) 
</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Interactions" type="InteractionsType"/>
	<xs:element name="InteractionUnstructured" type="xs:string"/>
	<xs:element name="Invasiveness" type="InvasivenessType">
		<xs:annotation>
			<xs:documentation>Information about invasive exotic species that could threaten to ecosystem, habitats and species</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InvasivenessAtomized" type="InvasivenessAtomizedType"/>
	<xs:element name="InvasivenessUnstructured" type="xs:string"/>
	<xs:element name="Keys" type="xs:string">
		<xs:annotation>
			<xs:documentation>Dichotomic or multientrance keys.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LegislationAtomized" type="LegislationAtomizedType"/>
	<xs:element name="Language" type="xs:string">
		<xs:annotation>
			<xs:documentation>The language of the taxon record, according to ISO 639.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LifeCycle" type="LifeCycleType">
		<xs:annotation>
			<xs:documentation>Life history of a living organism: The course of developmental changes in an organism from fertilized zygote to maturity or stages through which an organism passes. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LifeForm" type="LifeFormType">
		<xs:annotation>
			<xs:documentation>General appearance. Characteristic mode of growth or occurrence associated to its environment, particularly for plants. Comprising its size, shape, texture and orientation. Example: tree, shrubs, herbs. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LifeCycleAtomized" type="DetailType"/>
	<xs:element name="LifeCycleUnstructured" type="xs:string"/>
	<xs:element name="LifeFormAtomized" type="DetailType"/>
	<xs:element name="LifeFormUnstructured" type="xs:string"/>
	<xs:element name="Legislation" type="LegislationType"/>
	<xs:element name="LegislationUnstructured" type="xs:string">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
	</xs:element>
	<xs:element name="ManagementAndConservation" type="ManagementAndConservationType">
		<xs:annotation>
			<xs:documentation>Management: actions directed at conserving or restoring species / Conservation: interventions undertaken designed to preserve  specie </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ManagementAndConservationAtomized" type="ManagementAndConservationAtomizedType"/>
	<xs:element name="ManagementAndConservationUnstructured" type="xs:string"/>
	<xs:element name="MeasurementOrFact" type="MeasurementOrFactType"/>
	<xs:element name="Metadata" type="MetadataType"/>
	<xs:element name="Migratory" type="MigratoryType"/>
	<xs:element name="MigratoryAtomized" type="MigratoryAtomizedType"/>
	<xs:element name="MigratoryUnstructured" type="xs:string"/>
	<xs:element name="MiscDetails" type="MiscDetailType"/>
	<xs:element name="MolecularData" type="MolecularDataType">
		<xs:annotation>
			<xs:documentation>Information on the chemical structures and biological processes at the molecular level: DNA and proteins sequences, protein structures, expression profiles of genes protein domains, families of genes, mutations, polymorphisms, involvement in disease, ... Undeveloped. Placeholder for connecting with standards developed by specialists. For example, Standards for Data Exchange and Management from Scalalife (http://www.scalalife.eu/content/data)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MolecularDataAtomized" type="MolecularDataAtomizedType"/>
	<xs:element name="MolecularDataUnstructured" type="xs:string"/>
	<xs:element name="NaturalHistory" type="NaturalHistoryType"/>
	<xs:element name="NaturalHistoryUnstructured" type="xs:string"/>
	<xs:element name="NomenclatureAndClassification" type="NomenclatureAndClassificationType"/>
	<xs:element name="CommonNamesUnstructured" type="xs:string"/>
	<xs:element name="PopulationBiology" type="PopulationBiologyType"/>
	<xs:element name="PopulationBiologyAtomized" type="PopulationBiologyAtomizedType"/>
	<xs:element name="PopulationBiologyUnstructured" type="xs:string"/>
	<xs:element name="RecordMetadata" type="RecordMetadataType">
		<xs:annotation>
			<xs:documentation>This element includes the information about the Version, Revision,the language and target audiences of the Taxon Record.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ReproductionAtomized" type="DetailType"/>
	<xs:element name="Revision" type="RevisionType"/>
	<xs:element name="References" type="eol:referenceType"/>
	<xs:element name="Reproduction" type="ReproductionType">
		<xs:annotation>
			<xs:documentation>All data related to the production of offspring. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ReproductionUnstructured" type="xs:string"/>
	<xs:element name="SynonymsAtomized" type="SynonymsAtomizedType"/>
	<xs:element name="Synonyms" type="SynonymsType"/>
	<xs:element name="SynonymsUnstructured" type="xs:string"/>
	<xs:element name="TargetAudiences" type="TargetAudiencesType"/>
	<xs:element name="TaxonomicDescription" type="TaxonomicDescriptionType"/>
	<xs:element name="TaxonomicDescriptionUnstructured" type="xs:string"/>
	<xs:element name="TaxonRecord">
		<xs:annotation>
			<xs:documentation>Information about the record.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BaseElements">
					<xs:annotation>
						<xs:documentation>Basic information to identify the record.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RecordMetadata"/>
				<xs:element ref="NomenclatureAndClassification" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Standard information for every known taxon.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TaxonomicDescription" minOccurs="0"/>
				<xs:element ref="NaturalHistory" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Most relevant description about the species.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Invasiveness" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information about introduce exotic species that could threaten to ecosystem, habitats and species. Ref: www.cbd.int/doc/legal/cbd-es.pdf</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="HabitatAndDistribution" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Distribution of the species</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DemographyAndThreat" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information concerning the demographic aspects of the species, as well as the conservation status</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="UsesManagementAndConservation" minOccurs="0"/>
				<xs:element ref="associatedParty" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="dwc:MeasurementOrFact" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="References" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TaxonRecordName" type="TaxonRecordNameType"/>
	<xs:element name="TerritoryUnstructured" type="xs:string"/>
	<xs:element name="Territory" type="TerritoryType"/>
	<xs:element name="TerritoryAtomized" type="TerritoryAtomizedType"/>
	<xs:element name="ThreatStatusUnstructured" type="xs:string"/>
	<xs:element name="ThreatStatus" type="ThreatStatusType"/>
	<xs:element name="ThreatStatusAtomized" type="ThreatStatusAtomizedType"/>
	<xs:element name="UsesAtomized" type="UsesAtomizedType"/>
	<xs:element name="Uses" type="UsesType">
		<xs:annotation>
			<xs:documentation>Ways in which species are utilized by people </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UsesManagementAndConservation" type="UsesManagementAndConservationType"/>
	<xs:element name="UsesManagementAndConservationUnstructured" type="xs:string"/>
	<xs:element name="UsesUnstructured" type="xs:string">
		<xs:annotation>
			<xs:documentation>Uses in a unstructured format.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Version" type="VersionType"/>
	<!--  =============================================   -->
	<!--Datatypes-->
	<!--  =============================================   -->
	<xs:complexType name="AnnualCycleAtomizedType">
		<xs:sequence>
			<xs:element name="event">
				<xs:annotation>
					<xs:documentation>VC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startTimeInterval" minOccurs="0">
				<xs:annotation>
					<xs:documentation>VC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTimeInterval" minOccurs="0">
				<xs:annotation>
					<xs:documentation>VC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AnnualCyclesType">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="AnnualCycleAtomized" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation> </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="annualCycleUnstructured"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation> </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaseElementsType">
		<xs:sequence>
			<xs:element name="taxonRecordID" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identifier of the record within the database.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dwc:nameAccordingTo" minOccurs="0"/>
			<xs:element ref="dwc:taxonConceptID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An identifier for the taxonomic concept to which the record refers - not for the nomenclatural details of a taxon.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="globalUniqueIdentifier" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An Uniform Resource Name (URN) used as an unique identifier of the taxon record.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="abstract" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A brief summary of the most relevant or attractive features of this taxón to the general public. Could point to any kind of information. Example: Blue whale is the largest mammal in the world.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BehaviorType">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="BehaviorAtomized"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation> </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="BehaviorUnstructured"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation> </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommonNamesAtomizedType">
		<xs:sequence>
			<xs:element name="commonName" type="xs:string"/>
			<xs:element name="language" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The language of the taxon record, according to ISO 639.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="usedIn" type="DistributionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Location where the common name is used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="usedBy" type="xs:string" minOccurs="0"/>
			<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommonNamesType">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="CommonNamesAtomized" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="CommonNamesUnstructured"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DemographyAndThreatType">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element ref="Territory" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated mostly to vertebrates, referring to the territorial extension of the individual or group in terms of its activities (feeding, mating, etc.)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PopulationBiology" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Study of the variations in time and space as well as size and density of populations, and the factors that cause such variations.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ThreatStatus" minOccurs="0">
					<xs:annotation>
