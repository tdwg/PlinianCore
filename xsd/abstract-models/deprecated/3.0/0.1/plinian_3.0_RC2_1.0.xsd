			<xs:element name="TypeOfUse" type="NormalString"/>
			<xs:element name="Description" type="NormalString" minOccurs="0"/>
			<xs:element name="Region" type="NormalString" minOccurs="0"/>
			<xs:element name="References" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
