<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2013 (http://www.altova.com) by Santiago Martinez de la Riva (Real Jardín Botánico - CSIC) -->
<!--  publisher: Plinian Core Initiative  -->
<!--description: The Plinian Core v3.0 is a set of concepts that defines the necessary basic attributes to integrate and recover the information about species of organisms required by users specialized in biodiversity as well as users of other areas. The current version compiles a list of high-priority concepts selected by different groups of people interested in having apt mechanisms to access integrated information of species using Internet. -->
<!--issued:2007-03-16 -->
<!--modified:2013-04-12-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:plic="http://www.gbif.es/PliC/" xmlns:dc="http://purl.org/dc/terms/" xmlns:dcelem="http://purl.org/dc/elements/1.1/" xmlns:dwc="http://rs.tdwg.org/dwc/terms/" xmlns:abcd="http://www.tdwg.org/schemas/abcd/2.06" xmlns:tcs="http://www.tdwg.org/schemas/tcs/1.01" xmlns:simple="http://rs.tdwg.org/dwc/xsd/simpledarwincore/" xmlns:gisin="http://www.gisin.org/gisin/SpeciesStatus" xmlns:eol="http://www.eol.org/transfer/content/0.3" xmlns:eml="eml://ecoinformatics.org/eml-2.1.1">
	<xs:import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="http://dublincore.org/schemas/xmls/qdc/dc.xsd"/>
	<xs:import namespace="http://purl.org/dc/terms/" schemaLocation="http://darwincore.googlecode.com/svn/trunk/xsd/dublin_core.xsd"/>
	<xs:import namespace="http://www.tdwg.org/schemas/abcd/2.06" schemaLocation="http://rs.tdwg.org/abcd/2.06/ABCD_2.06.xsd"/>
	<xs:import namespace="http://www.tdwg.org/schemas/tcs/1.01" schemaLocation="http://www.tdwg.org/uploads/media/v101.xsd"/>
	<xs:import namespace="http://www.gisin.org/gisin/SpeciesStatus" schemaLocation="http://www.gisin.org/GISIN/SpeciesStatus_4_0_0.xsd"/>
	<xs:import namespace="http://rs.tdwg.org/dwc/terms/" schemaLocation="http://darwincore.googlecode.com/svn/trunk/xsd/tdwg_dwc_extensions.xsd"/>
	<xs:import namespace="eml://ecoinformatics.org/eml-2.1.1" schemaLocation="eml.xsd"/>
	<xs:import namespace="http://www.eol.org/transfer/content/0.3" schemaLocation="content_0_3.xsd"/>
	<xs:element name="Dataset">
		<xs:annotation>
			<xs:documentation>Root element from which all other elements will branch from</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Metadata">
					<xs:annotation>
						<xs:documentation>Information about the collections of records. This is an Extension of abcd: ContentMetadata.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TaxonRecord" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--  =============================================   -->
	<!--Elements-->
	<!--  =============================================   -->
	<xs:element name="Abstract">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AncillaryData" type="eol:dataObjectBaseType">
		<xs:annotation>
			<xs:documentation> From EOL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AnnualCycle" type="AnnualCycleType"/>
	<xs:element name="AnnualCycles" type="AnnualCyclesType"/>
	<xs:element name="AnnualCycleUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AtomizedDistribution" type="AtomizedDistributionType"/>
	<xs:element name="AudiencesUnstructured" type="xs:string">
		<xs:annotation>
			<xs:documentation>List audience codes separated by means of comma or /.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BaseElements">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TaxonRecordID" type="xs:string">
					<xs:annotation>
						<xs:documentation>Unique identifier of the record within the database.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="dwc:taxonConceptID">
					<xs:annotation>
						<xs:documentation>An identifier for the taxonomic concept to which the record refers - not for the nomenclatural details of a taxon.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="GlobalUniqueIdentifier" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>According to DarwinCore: An Uniform Resource Name (URN) used as an unique identifier of the taxon record.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Abstract" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A brief summary of the most relevant or attractive features of this taxón to the general public.  Could point to any kind of information. Example: Blue whale is the largest mammal in the world.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Behavior" type="DetailType">
		<xs:annotation>
			<xs:documentation>Responses, reactions or movements made by an organism in a particular situation.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Binary">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MIME-Type">
					<xs:annotation>
						<xs:documentation>Controlled Vocavulary: Application, Audio, Image, Message, Model, Multipart, Text and Video. http://www.iana.org/assignments/media-types/index.html</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Application"/>
							<xs:enumeration value="Audio"/>
							<xs:enumeration value="Image"/>
							<xs:enumeration value="Message"/>
							<xs:enumeration value="Model"/>
							<xs:enumeration value="Multipart"/>
							<xs:enumeration value="Text"/>
							<xs:enumeration value="Video"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:group ref="dcelem:elementsGroup"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BriefDescription">
		<xs:annotation>
			<xs:documentation>Brief description which reflect only the diagnostic characters, i.e. those that distinguish a taxon from other related or close taxa.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommonName" type="CommonNameType"/>
	<xs:element name="CommonNames" type="CommonNamesType"/>
	<xs:element name="DemographyAndThreat" type="DemographyAndThreatType"/>
	<xs:element name="DemographyAndThreatUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Detail" type="DetailType"/>
	<xs:element name="DetailUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Dispersal" type="DispersalType"/>
	<xs:element name="DispersalUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Distribution" type="DistributionType"/>
	<xs:element name="DistributionArea">
		<xs:annotation>
			<xs:documentation>Description of the distribution of the species.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EcologicalSignificance">
		<xs:annotation>
			<xs:documentation>Ecological importance of the taxon. (Placeholder)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="EcologicalUnit" type="DetailType"/>
				</xs:choice>
				<xs:element name="EcologicalUnstructured">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="para"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Endemic" type="EndemicType"/>
	<xs:element name="EnvironmentalEnvelope" type="EnvironmentalType"/>
	<xs:element name="EnvironmentalUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EndemismDescription" type="TextDescriptionType">
		<xs:annotation>
			<xs:documentation>List of areas presented in an unstructured and categorized the degree of safety.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Feeding" type="FeedingType"/>
	<xs:element name="FeedingUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Folklore" type="FolkloreType"/>
	<xs:element name="FolkloreUnit" type="FolkloreUnitType"/>
	<xs:element name="FolkloreUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FullDescription">
		<xs:annotation>
			<xs:documentation>Complete and detailed account of the taxonomic and systematic characters, i.e. those that define and allow to recognize an organism.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="DescriptionItem" type="DescriptionItemType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Structured diagnostic characters.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FullDescriptionText">
					<xs:annotation>
						<xs:documentation>Unstructured diagnostic characters.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="FullDescriptionText">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Habitat" type="HabitatType"/>
	<xs:element name="HabitatAndDistribution" type="HabitatAndDistributionType"/>
	<xs:element name="HabitatAndDistributionUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Habitats" type="HabitatsType"/>
	<xs:element name="HabitatUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Hierarchy" type="HierarchyType"/>
	<xs:element name="Interaction" type="InteractionType"/>
	<xs:element name="Interactions" type="InteractionsType"/>
	<xs:element name="InteractionUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Invasiveness" type="InvasivenessType"/>
	<xs:element name="InvasivenessUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Keys" type="TextDescriptionType">
		<xs:annotation>
			<xs:documentation>Dichotomic  or multientrance keys.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Laws" type="LawsType"/>
	<xs:element name="LifeCycle" type="DetailType">
		<xs:annotation>
			<xs:documentation>Life history of a living organism: The course of developmental changes in an organism from fertilized zygote to maturity or stages through which an organism passes. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LifeForm" type="DetailType">
		<xs:annotation>
			<xs:documentation>General appearance. Characteristic mode of growth or occurrence associated to its environment, particularly for plants. Comprising its size, shape, texture and orientation. Example: tree, shrubs, herbs. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Legislation" type="LegislationType"/>
	<xs:element name="LegislationUnstructured" type="TextDescriptionType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
	</xs:element>
	<xs:element name="Management" type="ManagementType"/>
	<xs:element name="ManagementUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MeasurementOrFact" type="MeasurementOrFactType"/>
	<xs:element name="Metadata" type="MetadataType"/>
	<xs:element name="Migratory" type="MigratoryType"/>
	<xs:element name="Migration" type="MigrationType"/>
	<xs:element name="MigratoryUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MiscDetails" type="MiscDetailType"/>
	<xs:element name="MolecularData" type="MolecularDataType">
		<xs:annotation>
			<xs:documentation>Information on the chemical structures and biological processes at the molecular level: DNA and proteins sequences, protein structures, expression profiles of genes protein domains, families of genes,  mutations, polymorphisms, involvement in disease, ... Undeveloped. Placeholder for connecting with standards developed by specialists. For example, Standards for Data Exchange and Management from Scalalife (http://www.scalalife.eu/content/data)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MolecularDataUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NaturalHistory" type="NaturalHistoryType"/>
	<xs:element name="NaturalHistoryUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NomenclatureAndClassification" type="NomenclatureAndClassificationType"/>
	<xs:element name="NameUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PopulationBiology" type="PopulationBiologyType"/>
	<xs:element name="PopulationBiologyUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RecordMetadata">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Language" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The language of the taxon record, according to ISO 639.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TargetAudiences" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Users to which the information is addressed. The defined audiences are: Biodiversity researchers, teachers and students, decision makers, professionals from another areas and general public.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Version" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Number and date of the current version.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Revision"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReproductionUnit" type="DetailType"/>
	<xs:element name="Revision" type="RevisionType"/>
	<xs:element name="References" type="eol:referenceType">
		<xs:annotation>
			<xs:documentation> From EOL</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Reproduction" type="ReproductionType">
		<xs:annotation>
			<xs:documentation>All data related to the production of offspring. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Synonym" type="SynonymType"/>
	<xs:element name="Synonyms" type="SynonymsType"/>
	<xs:element name="SynonymsUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TargetAudiences">
		<xs:complexType>
			<xs:choice>
				<xs:element name="Audience" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Repetitive concept where the target audiences are written one by one. (Predetermined list:						01. Expert (High).							02. Knowledgeable (Medium).							03. General (Low).
									</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Expert">
								<xs:annotation>
									<xs:documentation>Professionals on Biodiversity (researchers, decision makers, students of higher levels, etc.). By training and experience they have a good knowledge of scientific terminology and taxonomic and nomenclatural issues.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Knowledgeable">
								<xs:annotation>
									<xs:documentation>Public interest in the knowledge of living organisms but whose training and experience is not directly linked to knowledge of biodiversity, or to be enjoined, their training has not been directly related to biodiversity (professionals from other areas of knowledge, students, amateurs, etc.). Your level of interest is high but not usually use and know the terminology, taxonomy and scientific nomenclature.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="General">
								<xs:annotation>
									<xs:documentation>General Public (anyone interested in living organisms). Persons interested on biodiversity, frequently or timely, but lack the training, expertise or basic knowledge on terminology, taxonomy or scientific nomenclature.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="AudiencesUnstructured">
					<xs:annotation>
						<xs:documentation>List audience codes separated by means of comma or /.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="TaxonomicalDescription" type="TaxonomicalDescriptionType"/>
	<xs:element name="TaxonomicalDescriptionUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TaxonRecord">
		<xs:annotation>
			<xs:documentation>Information about the record.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BaseElements">
					<xs:annotation>
						<xs:documentation>Basic information to identify the record.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RecordMetadata"/>
				<xs:element ref="NomenclatureAndClassification" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Standard information for every known taxon.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TaxonomicalDescription" minOccurs="0"/>
				<xs:element ref="NaturalHistory" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Most relevant description about the species.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Invasiveness" minOccurs="0"/>
				<xs:element ref="HabitatAndDistribution" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Distribution of the species</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DemographyAndThreat" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information concerning the demographic aspects of the species, as well as the conservation status</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="UsesManagementAndConservation" minOccurs="0"/>
				<xs:element ref="References" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TaxonRecordName">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ScientificName" type="tcs:ScientificName">
					<xs:annotation>
						<xs:documentation>Complete name of the taxon.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerritoryUnstructured">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="para"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Territory" type="TerritoryType"/>
	<xs:element name="ThreatStatus">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ThreatStatus" type="xs:string">
					<xs:annotation>
						<xs:documentation>Description of the threat status.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Authority" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Criteria apply to assess such status.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AppliesTo" type="DistributionType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Location where the status is used.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Use" type="UseType"/>
	<xs:element name="Uses" type="UsesType"/>
	<xs:element name="UsesManagementAndConservation" type="UsesManagementAndConservationType"/>
	<xs:element name="UsesUnstructured" type="TextDescriptionType">
		<xs:annotation>
			<xs:documentation>Uses in a unstructured format.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Value" type="xs:string"/>
	<xs:element name="Version" type="VersionType"/>
	<!--  =============================================   -->
	<!--Datatypes-->
	<!--  =============================================   -->
	<xs:complexType name="AnnualCycleType">
		<xs:sequence>
			<xs:element name="Event"/>
			<xs:element name="TimeInterval"/>
			<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AnnualCyclesType">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="AnnualCycle" maxOccurs="unbounded"/>
				<xs:element ref="AnnualCycleUnstructured"/>
			</xs:choice>
			<xs:element ref="AncillaryData" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AtomizedDistributionType">
		<xs:sequence>
			<xs:element name="Catalogue" type="xs:string">
				<xs:annotation>
					<xs:documentation>Catalogue where the GeographicEntity is documented.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="GeographicEntity" type="xs:string">
					<xs:annotation>
						<xs:documentation>Name of the geographic entity.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="State" type="xs:string"/>
